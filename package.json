{
  "name": "magix",
  "displayName": "Magix",
  "description": "Magix 开发增强套件 @MMFE",
  "version": "0.3.0",
  "publisher": "fengyu",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mx.plugin",
    "onLanguage:handlebars",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:es"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "mx.jumper.toHtml",
        "title": "magix-跳转至html"
      },
      {
        "command": "mx.jumper.toES",
        "title": "magix-跳转至js/ts"
      },
      {
        "command": "mx.jumper.backAndForth",
        "title": "MX"
      },
      {
        "command": "mx.test",
        "title": "MX Test"
      }
    ],
    "keybindings": [
      {
        "command": "mx.jumper.backAndForth",
        "key": "alt+tab",
        "mac": "alt+tab",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId ==  typescript",
          "command": "mx.jumper.toHtml",
          "group": "0_mx"
        },
        {
          "when": "resourceLangId ==  javascript",
          "command": "mx.jumper.toHtml",
          "group": "0_mx"
        },
        {
          "when": "resourceLangId ==  html",
          "command": "mx.jumper.toES",
          "group": "0_mx"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "magix",
          "title": "mx-plugin",
          "icon": "images/Magix.svg"
        }
      ]
    },
    "views": {
      "magix": [
        {
          "id": "magix",
          "name": "插件设置"
        }
      ]
    },
    "grammars": [
      {
        "language": "handlebars",
        "scopeName": "text.html.mxtpl",
        "path": "./syntaxes/mxtpl.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "es",
        "aliases": [
          "es",
          "ES"
        ],
        "extensions": [
          ".es"
        ]
      },
      {
        "id": "handlebars",
        "aliases": [
          "mxtpl",
          "handlebars"
        ],
        "extensions": [
          ".tpl",
          ".html"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "@types/parse5": "^5.0.0",
    "request": "^2.81.0",
    "babylon": "^6.18.0",
    "map-lru": "^1.0.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21"
  },
  "dependencies": {
    "@types/css-tree": "^1.0.0",
    "@types/parse5": "^5.0.0",
    "babylon": "^6.18.0",
    "css-tree": "^1.0.0-alpha.29",
    "datauri": "^1.1.0",
    "map-lru": "^1.0.2",
    "opn": "^4.0.2",
    "request": "^2.81.0"
  },
  "repository": "https://github.com/thx/vscode-magix",
  "author": "",
  "license": "ISC"
}
